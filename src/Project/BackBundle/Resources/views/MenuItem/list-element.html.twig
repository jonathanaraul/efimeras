<tr id="fila_{{element.id}}">
	<td >{{element.id}}</td>
	<td >{{element.name}}</td>
	<td >{{element.rank}}</td>
	<td >
		{%if element.tipo==0%}
		Página
		{%elseif element.tipo==1%}
		Categoría
		{%else%}
		SPACER
		{%endif%}
    </td>
	<td >
		{%if element.tipo==0%}
		{{element.page.name}}
		{%elseif element.tipo==1%}
		{{element.category.name}}
		{%else%}
		SPACER
		{%endif%}
    </td>
	<td class="hidden-phone">{{element.created.format('d/m/Y H:i:s')}}</td>
	<td class="hidden-phone">{{element.updated.format('d/m/Y H:i:s')}}</td>

    {% include 'ProjectBackBundle:Helpers:listado-status-edit-delete.html.twig'  with {'tipo':'MenuItem','rutaEditar':'project_back_menu_item_edit'}  %}
</tr>