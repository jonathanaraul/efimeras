<tr id="fila_{{element.id}}">
	<td >{{element.id}}</td>
	<td >{{element.name}}</td>
	<td >{{element.description}}</td>
	<td >
	<ul>
		{%for item in element.menuItems%}
		<li>
			{{item.name}}
		</li>
		{%endfor%}
	</ul>
    </td>

	<td class="hidden-phone">{{element.created.format('d/m/Y H:i:s')}}</td>
	<td class="hidden-phone">{{element.updated.format('d/m/Y H:i:s')}}</td>

	{% include 'ProjectBackBundle:Helpers:listado-status-edit-delete.html.twig'  with {'tipo':'Menu','rutaEditar':'project_back_menu_edit'}  %}
</tr>